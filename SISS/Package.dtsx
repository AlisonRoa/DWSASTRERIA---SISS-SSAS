                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    CONVERT(DATETIME, p.fechapedido) AS fecha,
    DATEPART(YEAR, p.fechapedido) AS anio,
    CONVERT(NVARCHAR(15), FORMAT(p.fechapedido, 'MMMM', 'es-ES')) AS mes_nombre,
    CONVERT(NVARCHAR(15), FORMAT(p.fechapedido, 'dddd', 'es-ES')) AS dia_nombre,
    DATEPART(DAYOFYEAR, p.fechapedido) AS dia_num,
    DATEPART(MONTH, p.fechapedido) AS mes_num,
    CASE 
        WHEN DATEPART(MONTH, p.fechapedido) BETWEEN 1 AND 3 THEN 1
        WHEN DATEPART(MONTH, p.fechapedido) BETWEEN 4 AND 6 THEN 2
        WHEN DATEPART(MONTH, p.fechapedido) BETWEEN 7 AND 9 THEN 3
        WHEN DATEPART(MONTH, p.fechapedido) BETWEEN 10 AND 12 THEN 4
        ELSE NULL
    END AS trimestre,
    CASE 
        WHEN DATEPART(MONTH, p.fechapedido) BETWEEN 1 AND 6 THEN 1
        WHEN DATEPART(MONTH, p.fechapedido) BETWEEN 7 AND 12 THEN 2
        ELSE NULL
    END AS semestre
FROM Pedidos AS p

UNION ALL

SELECT 
    CONVERT(DATETIME, p.fechaentrega) AS fecha,
    DATEPART(YEAR, p.fechaentrega) AS anio,
    CONVERT(NVARCHAR(15), FORMAT(p.fechaentrega, 'MMMM', 'es-ES')) AS mes_nombre,
    CONVERT(NVARCHAR(15), FORMAT(p.fechaentrega, 'dddd', 'es-ES')) AS dia_nombre,
    DATEPART(DAYOFYEAR, p.fechaentrega) AS dia_num,
    DATEPART(MONTH, p.fechaentrega) AS mes_num,
    CASE 
        WHEN DATEPART(MONTH, p.fechaentrega) BETWEEN 1 AND 3 THEN 1
        WHEN DATEPART(MONTH, p.fechaentrega) BETWEEN 4 AND 6 THEN 2
        WHEN DATEPART(MONTH, p.fechaentrega) BETWEEN 7 AND 9 THEN 3
        WHEN DATEPART(MONTH, p.fechaentrega) BETWEEN 10 AND 12 THEN 4
        ELSE NULL
    END AS trimestre,
    CASE 
        WHEN DATEPART(MONTH, p.fechaentrega) BETWEEN 1 AND 6 THEN 1
        WHEN DATEPART(MONTH, p.fechaentrega) BETWEEN 7 AND 12 THEN 2
        ELSE NULL
    END AS semestre
FROM Pedidos AS p</property>
